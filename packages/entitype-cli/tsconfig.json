{
  "version": "2.1.1",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "dist",
    "paths": {
      "entitype": [
        "../entitype"
      ],
      "entitype/*": [
        "../entitype/*"
      ],
      "entitype-mysql": [
        "../entitype-mysql"
      ],
      "entitype-mysql/*": [
        "../entitype-mysql/*"
      ]
    }
  },
  "exclude": [
    "test",
    "tmp",
    "temp",
    "build",
    "dist",
    "node_modules",
    "**/*.spec.ts"
  ]
}
