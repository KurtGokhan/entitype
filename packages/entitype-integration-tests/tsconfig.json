{
  "version": "2.1.1",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "outDir": "build",
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "../../*"
      ],
      "entitype": [
        "./node_modules/entitype"
      ],
      "entitype-mysql": [
        "./node_modules/entitype-mysql"
      ],
      "entitype-websql": [
        "./node_modules/entitype-websql"
      ],
      "common/*": [
        "../common/*"
      ]
    }
  },
  "exclude": [
    "tmp",
    "temp",
    "build",
    "dist",
    "node_modules"
  ]
}
