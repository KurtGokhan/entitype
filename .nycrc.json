{
  "check-coverage": true,
  "include": [
    "packages/entitype/src/**/*.ts",
    "packages/entitype-websql/src/**/*.ts",
    "packages/entitype-mysql/src/**/*.ts"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "require": [
    "ts-node/register/type-check"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "report-dir": "./tmp",
  "temp-directory": "./tmp/.nyc_output",
  "all": true
}
